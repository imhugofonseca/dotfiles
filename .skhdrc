:: default   : yabai -m config active_window_opacity       1.0
:: focus  @ : yabai -m config active_window_opacity       1.0

default                                 < ctrl - space      ; focus
focus                                   < ctrl - space      ; default
focus                                   < escape            ; default

# focus keybinds
focus < c : yabai -m space --focus 4
focus < b : yabai -m space --focus 3
focus < t : yabai -m space --focus 5
focus < m : yabai -m space --focus 1
focus < f : yabai -m space --focus 2